class main{
    public static void main(String[] args){
        int[] arr = {2,4,7,9,12};
        
        List<Integer> odd = new ArrayList<>();
        List<Integer> even = new ArrayList<>();
        
        for(int i = 0 ; i < arr.length ; i++){
            if(arr[i]%2==0){
                even.add(arr[i]);
            }
            else{
                odd.add(arr[i]);
            } 
        }
         System.out.println(even);
         System.out.println(odd);
    }
}
